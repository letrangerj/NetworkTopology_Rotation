    "modularity_replicates": {
        "pilot": 50,
        "final": 200
    },
    "mc_cores": 4,
    "parallel": {
        "mc_cores": 4
    },
    "null_model_N": {
        "pilot": 100,
        "final": 1000
    },
    "swaps_per_null": "10 * edge_count",
    "pyov_representation": "93_node",
    "use_optional_nulls": false,
    "remove_largest_FG_for_sensitivity": true,
    "results_dir": "results/phase_04/",
    "primary_outputs": [
        "results/phase_04_topology_summary.rds",
