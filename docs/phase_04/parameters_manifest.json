        "package_versions": {
            "bipartite": "<packageVersion('bipartite')>",
            "igraph": "<packageVersion('igraph')>",
            "vegan": "<packageVersion('vegan')>",
            "dplyr": "<packageVersion('dplyr')>",
            "ggplot2": "<packageVersion('ggplot2')>",
            "R.matlab": "<packageVersion('R.matlab')>"
        }
    },
    "binary_analysis": true,
    "weighted_analysis": false,
    "zero_denominator_policy": "NA_and_flag (set ratio to null/NA and add a flag column in syn_rec table)",
    "modularity_consensus_method": "replicate_consensus_with_coassignment_matrix",
    "manifest_created_by": "agent",
